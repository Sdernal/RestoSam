<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sdern_000.restosam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".MenuActivity" />
        <activity
            android:name=".RestaurantActivity"
            android:label="@string/title_activity_restaurant"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.sdern_000.restosam.MainActivity" />
        </activity>
        <activity android:name=".ExtendedSearchRestaurantActivity" />
        <activity android:name=".SearchRestaurantActivity" />
        <activity android:name=".RestaurantSearchResultActivity" />
        <activity android:name=".BookActivity"></activity>
    </application>

</manifest>